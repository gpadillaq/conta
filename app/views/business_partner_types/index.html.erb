<p id="notice"><%= notice %></p>

<h1>Business Partner Types</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @business_partner_types.each do |business_partner_type| %>
      <tr>
        <td><%= business_partner_type.descripcion %></td>
        <td><%= link_to 'Show', business_partner_type %></td>
        <td><%= link_to 'Edit', edit_business_partner_type_path(business_partner_type) %></td>
        <td><%= link_to 'Destroy', business_partner_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Business Partner Type', new_business_partner_type_path %>
